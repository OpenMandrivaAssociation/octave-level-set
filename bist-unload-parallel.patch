+++ octave-level-set-0.3.1~git-2019-04-13/inst/so_init_params.m
@@ -71,7 +71,7 @@ endfunction
 %!test
 %!  p = so_init_params (true);
 %!error <'pararrayfun' is not available>
-%!  pkg unload parallel;
+%!  pkg unload -nodeps parallel;
 %!  p = so_init_params (true, 2);
 %!test
 %!  pkg load parallel;
